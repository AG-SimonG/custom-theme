<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
     <!-- === Home Page === -->
     <record id="services_page" model="website.page">
         <field name="name">Home page</field>
         <field name="website_published">True</field>
         <field name="url">/</field>
         <field name="type">qweb</field>
         <field name="key">theme_tutorial.home_page</field>
         <field name="arch" type="xml">
             <t t-name="theme_tutorial.home_page_template">
                <t t-call="website.layout">
                    <div id="wrap">
                        <div class="container">
                            <h1>Home</h1>
                            <h2>Our services include :</h2>
                            <ul class="services">
                                <li>Cloud </li>
                                <li>Support</li>
                                <li>Storage</li>
                            </ul>
                        </div>
                    </div>
                </t>
             </t>
         </field>
     </record>

 </odoo>